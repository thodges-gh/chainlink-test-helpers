{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "baseUrl": "./",
    "target": "es5",
    "outDir": "./dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
